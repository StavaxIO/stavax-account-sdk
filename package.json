    "axios": "^1.7.2",
    "viem": "2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@types/node": "^17.0.35",
    "@types/telegram-web-app": "^7.2.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "clean": "rm -rf lib && rm -rf es &&  rm -rf dist",
    "package": "echo '{ \"type\": \"commonjs\" }' > lib/package.json",
    "build": "yarn clean && node ./esBuild.js && tsc --emitDeclarationOnly -p tsconfig.json && yarn package",
    "dev:lib": "cross-env NODE_ENV=development concurrently \"tsc -w\" \"node ./esBuild.js\"",
    "release:minor": "yarn version minor && yarn build",
    "release:patch": "yarn version patch && yarn build"
  },
  "volta": {
    "node": "20.14.0",
    "yarn": "4.3.1"
  }
