  "repository": {
    "type": "git",
    "url": "git+https://github.com/StavaxIO/stavax-account-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wagmi/connectors": "^5.0.15",
